upstream finch-rest {
  least_conn;
  {{range service "finch-REST"}}server {{.Address}}:{{.Port}};
  {{end}}    
}

server {
  listen 80 default_server;
  charset utf-8;
  location /api/get {
    proxy_pass http://finch-rest/get;
  } 
  location /api/hash {
    proxy_pass http://finch-rest/hash;
  } 
}